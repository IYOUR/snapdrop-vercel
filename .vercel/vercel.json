{
  "version": 2,
  "builds": [
	{ "src": "public/**", "use": "@vercel/static" },
    { "src": "api/**", "use": "@vercel/node" }
  ],
  "routes": [
	{
	  "src": "api/server/(.*)",
	  "dest": "http://localhost:3000/$1",
	  "headers": {
		"Connection": "Upgrade",
		"Upgrade": "websocket"
	  }
	}
  ]
}
