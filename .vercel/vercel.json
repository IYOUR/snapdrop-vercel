{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server/:1"
    },
    {
      "handle": "filesystem",
      "src": "/public/(.*)",
      "dest": "/public/:1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    },
	{
      "src": "/server/(.*)",
      "dest": "http://localhost:3000/server/$1"
    }
  ]
}
